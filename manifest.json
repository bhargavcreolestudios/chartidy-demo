{
  "name": "Chartidy",
  "version": "1.0",
  "description": "Display the chart of table on user choice.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www1.nseindia.com/*"],
      "css": ["src/style.css"],
      "js": ["src/scripts/jquery.js", "src/scripts/canvas.js", "src/contentScript.js"]
    },
    {
      "matches": ["https://in.finance.yahoo.com/*"],
      "css": ["src/style.css"],
      "js": ["src/scripts/jquery.js", "src/scripts/canvas.js", "src/contentYahooScript.js"]
    }
  ]
}
